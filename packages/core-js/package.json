{
  "name": "@zerobias-org/types-core-js",
  "version": "1.0.2",
  "description": "JS core type implementations.",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist/src/*.js",
      "require": "./dist/src/*.js",
      "default": "./dist/src/*.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist/ .docs/ generated/",
    "generate": "npm run generate:code",
    "transpile": "tsc",
    "build": "npm run generate && npm run transpile",
    "test": "mocha --inline-diffs --reporter=list test/unit/**/*.ts",
    "test:unit": "mocha --inline-diffs --reporter=list test/unit/*.ts",
    "test:watch": "npm run test -- --watch --reporter=min",
    "docs:tsdoc": "typedoc --plugin typedoc-plugin-markdown --out .docs",
    "docs:combine": "concat-md --toc --decrease-title-levels --dir-name-as-title .docs > USERGUIDE.md",
    "docs": "npm run docs:tsdoc && npm run docs:combine",
    "version": "npm run generate:badge && git add version.svg",
    "generate:badge": "npx badge $(yq -oy e .name package.json) $(npm run -s get-version) :red @flat > version.svg",
    "generate:code": "JAVA_OPTS=\"${JAVA_OPTS} -DcoreTypesSource=../../src\" hub-generator generate -g hub-module --additional-properties supportsES6=true -p skipHubImpl=true -p isCore=true -i ../../node_modules/@zerobias-org/types-core/models.yml -o generated/ && ./bin/fix-generated-esm.sh generated",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "get-version": "node -p \"require('./package.json').version\""
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "author": "team@zerobias.com",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:zerobias-org/types.git",
    "directory": "packages/core-js"
  },
  "publishConfig": {
    "registry": "https://pkg.zerobias.org/"
  },
  "files": [
    "dist/src",
    "dist/generated",
    "*.md",
    "bom.json"
  ],
  "dependencies": {
    "@supercharge/promise-pool": "3.2.0",
    "@zerobias-org/types-core": "^1.0.2",
    "awesome-phonenumber": "7.6.0",
    "axios": "^1.13.2",
    "buffer": "6.0.3",
    "cron-parser": "5.4.0",
    "ip-address": "10.1.0",
    "pluralize": "8.0.0",
    "semver": "7.7.3",
    "tinyduration": "3.4.1",
    "uuid": "13.0.0",
    "validator": "13.15.23",
    "vuln-vects": "1.1.0"
  }
}
